{
  "version": "1.0.11",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:coverage": "tsdx test --coverage",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint src",
    "lint:fix": "tsdx lint src --fix",
    "lint:report": "tsdx lint src --report-file report.json",
    "fmt": "prettier --write **/*.{js,ts}",
    "prepare": "tsdx build"
  },
  "peerDependencies": {
    "@rtcts/isomorphic": "^1.4.9",
    "@rtcts/utils": "^1.4.1",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src"
    }
  },
  "lint-staged": {
    "*.(ts|json)": [
      "npm run fmt",
      "git add"
    ],
    "*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "name": "@rtcts/ishop-shared",
  "description": "Entities, enums, sets for ishop project",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "module": "dist/ishop-shared.esm.js",
  "devDependencies": {
    "@rtcts/isomorphic": "^1.4.9",
    "@rtcts/utils": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "prettier": "^2.0.5",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  }
}
