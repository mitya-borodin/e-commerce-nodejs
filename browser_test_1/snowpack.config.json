{
  "extends": "@snowpack/app-scripts-svelte",
  "scripts": {
    "build:css": "postcss",
    "run:lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "run:lint::watch": "watch \"$1\" src",
    "run:tsc": "tsc --noEmit",
    "run:tsc::watch": "$1 --watch"
  },
  "plugins": ["@snowpack/plugin-dotenv"]
}
